<h1>Listing empresas</h1>

<table>
  <tr>
    <th>Detalle</th>
    <th>Contacto</th>
    <th>Telefono</th>
    <th>Email</th>
    <th>Observaciones</th>
    <th></th>
  </tr>

<% @empresas.each do |empresa| %>
  <tr>
    <td><%= empresa.detalle %></td>
    <td><%= empresa.contacto%></td>
    <td><%= empresa.telefono%></td>
    <td><%= empresa.email%></td>
    <td><%= empresa.observaciones%></td>
    <td><%= link_to 'Ver', empresa %></td>
    <td><%= link_to 'Editar', edit_empresa_path(empresa) %></td>
    <td><%= link_to 'Borrar', empresa, :confirm => 'Esta Seguro?', :method => :delete %></td>
    <td><%= link_to 'Proyectos', empresa_projects_path(empresa) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nueva Empresa', new_empresa_path %>
